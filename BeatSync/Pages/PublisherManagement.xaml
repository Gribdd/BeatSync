<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BeatSync.Pages.PublisherManagement"
             Title="PublisherManagement"
             NavigationPage.HasNavigationBar="False"
             BackgroundImageSource="resizedbackgroundpixel.png">
    
    <VerticalStackLayout>
        <HorizontalStackLayout>
            <ImageButton Source="profileicon.png" HeightRequest="35" Margin="30" HorizontalOptions="Start"></ImageButton>
            <Label Style="{StaticResource SubHeadline}" Text="Publisher Management" TextColor="White" Margin="-25,35,0,0"></Label>
            <ImageButton Source="search.png" HeightRequest="35" Margin="0,0,0,0" HorizontalOptions="Start"></ImageButton>
            <ImageButton Source="add.png" HeightRequest="30" Margin="-5,0,0,0" HorizontalOptions="Start"></ImageButton>
        </HorizontalStackLayout>
    </VerticalStackLayout>
</ContentPage>